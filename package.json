		"prettier": "^2.8.8",
		"rimraf": "^5.0.1",
		"run-p": "^0.0.0",
		"style-loader": "^3.3.3",
		"supertest": "^6.3.3",
		"tailwindcss": "^3.3.2",
		"ts-loader": "^9.4.3",
		"ts-morph": "^18.0.0",
