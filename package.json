		"eslint": "^8.42.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-import": "^2.27.5",
		"fancy-log": "^2.0.0",
		"gulp": "^4.0.2",
		"gulp-clean": "^0.4.0",
		"gulp-sourcemaps": "^3.0.0",
		"gulp-typescript": "^6.0.0-alpha.1",
		"gulp-uglify": "^3.0.2",
		"html-webpack-plugin": "^5.5.3",
		"husky": "^8.0.3",
		"lerna": "^7.0.2",
