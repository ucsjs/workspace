{
	"name": "@ucsjs/workspace",
	"version": "0.1.0",
	"description": "UCS.js (@workspace)",
	"main": "dist/main.js",
	"private": false,
	"scripts": {
		"prebuild": "rimraf dist",
		"build:server": "tsc",
		"build:editor": "webpack --config webpack.config.js",
		"build:css": "tailwindcss -i ./editor/assets/tailwind.css -o ./editor/assets/tailwind.min.css --quiet --minify --no-warnings",
		"build:packages": "rm -rf ./packages/**/*.tsbuildinfo && tsc -b -v packages && gulp move",
		"build": "npm-run-all --parallel build:*",
		"dev:server": "export NODE_ENV=dev && ts-node -r tsconfig-paths/register ./src/index.ts",
		"dev:editor": "webpack --config webpack.config.js --watch > /dev/null 2>&1",
		"dev:css": "tailwindcss -i ./editor/assets/tailwind.css -o ./editor/assets/tailwind.min.css --quiet --minify --no-warnings > /dev/null 2>&1",
		"dev": "yarn clean && npm-run-all --parallel dev:*",
		"generate:docs": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./src/docs.ts",
		"clean:tsfiles": "find ./src -name \"*.d.ts,*.js\" -delete",
		"clean:packages": "find ./packages -name \"*.d.ts,*.js\" -delete",
		"clean": "yarn clean:tsfiles && yarn clean:packages > /dev/null 2>&1",
		"start": "export NODE_ENV=prod && node dist/index.js",
		"blueprint": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./src/run.ts",
		"uibuilder": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./src/uibuilder.ts",
		"format": "prettier \"**/*.ts\" \"packages/**/*.json\" --ignore-path ./.prettierignore --write && git status",
		"test": "mocha packages/**/*.spec.ts",
		"test:dev": "mocha -w --watch-files \"packages\" packages/**/*.spec.ts",
		"test:docker:up": "docker-compose -f integration/docker-compose.yml up -d",
		"test:docker:down": "docker-compose -f integration/docker-compose.yml down",
		"lint": "concurrently 'npm run lint:packages' 'npm run lint:integration' 'npm run lint:spec'",
		"lint:fix": "concurrently 'npm run lint:packages -- --fix' 'npm run lint:integration -- --fix' 'npm run lint:spec -- --fix'",
		"lint:integration": "eslint 'integration/*/{,!(node_modules)/**/}/*.ts'",
		"lint:packages": "eslint 'packages/**/**.ts' --ignore-pattern 'packages/**/*.spec.ts'",
		"lint:spec": "eslint 'packages/**/**.spec.ts'",
		"prepare": "husky install"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ucsjs/workspace.git"
	},
	"author": "Andre Ferreira",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ucsjs/workspace/issues"
	},
	"homepage": "https://github.com/ucsjs/workspace#readme",
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">= 14.0.0"
	},
	"lint-staged": {
		"**/*.ts": [
			"prettier --ignore-path ./.prettierignore --write"
		],
		"packages/**/*.json": [
			"prettier --ignore-path ./.prettierignore --write"
		]
	},
	"mocha": {
		"require": [
			"ts-node/register",
			"tsconfig-paths/register",
			"node_modules/reflect-metadata/Reflect.js",
			"hooks/mocha-init-hook.ts"
		],
		"exit": true
	},
	"devDependencies": {
		"@types/chai": "^4.3.5",
		"@types/chai-as-promised": "^7.1.5",
		"@types/cors": "^2.8.13",
		"@types/cron": "^2.0.1",
		"@types/express": "^4.17.17",
		"@types/fs-extra": "^11.0.1",
		"@types/globby": "^9.1.0",
		"@types/jsonwebtoken": "^9.0.2",
		"@types/mocha": "^10.0.1",
		"@types/multer": "^1.4.7",
		"@types/node": "^20.2.6",
		"@types/supertest": "^2.0.12",
		"@types/ws": "^8.5.5",
		"@types/yaml": "^1.9.7",
		"@typescript-eslint/eslint-plugin": "^5.59.9",
		"@typescript-eslint/parser": "^5.59.9",
		"@vue/cli-plugin-typescript": "^5.0.8",
		"@vue/eslint-config-prettier": "^7.1.0",
		"@vue/eslint-config-typescript": "^11.0.3",
		"@vue/test-utils": "^2.3.2",
		"@vue/tsconfig": "^0.4.0",
		"autoprefixer": "^10.4.14",
